{
  "kind": "collectionType",
  "collectionName": "listings",
  "info": {
    "name": "Listing",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Rating": {
      "type": "integer"
    },
    "Notes": {
      "type": "richtext"
    },
    "Started_At": {
      "type": "date"
    },
    "Finished_At": {
      "type": "date"
    },
    "Image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "filter": {
      "model": "filter",
      "via": "listings"
    },
    "tags": {
      "collection": "tags",
      "via": "listing"
    }
  }
}
